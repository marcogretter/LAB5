#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define N 50

int main()
{
    char str1[N],str2[N];
    printf("Inserisci prima parola:\n");
    scanf("%s",str1);
    printf("Inserisci seconda parola:\n");
    scanf("%s",str2);
    if(strcmp(str1,str2)>0)
        printf("%s è seguita alfabeticamente da %s\n",str2,str1);
    else
        printf("%s è seguita alfabeticamente da %s\n",str1,str2);
    return 0;
}
